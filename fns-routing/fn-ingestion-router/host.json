{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "eventHubs": {
      "maxEventBatchSize": 32
    }
  },
  "logging": {
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Host": "Error",
      "Worker": "Warning",
      "Microsoft": "None"
    }
  }
}
